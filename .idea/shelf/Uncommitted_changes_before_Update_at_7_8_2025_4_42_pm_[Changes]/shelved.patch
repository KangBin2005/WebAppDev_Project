+# In your imports section
+from datetime import datetime
+
+# Notification class (add to your models)
+class Notification:
+    def __init__(self, user_id, message, notification_type, related_id=None):
+        self.notification_id = 0  # Will be set when saving
+        self.user_id = user_id
+        self.message = message
+        self.notification_type = notification_type  # 'new_activity', 'reminder', 'enquiry_reply'
+        self.related_id = related_id  # ID of related activity/enquiry
+        self.created_at = datetime.now()
+        self.read = False
+
+    def mark_as_read(self):
+        self.read = True
